            program { name: "edge_top_hide";
                signal: "edge,hide";
                source: "edge,top";
                action: STATE_SET "hidden" 0.0;
                transition: LINEAR 0.2;
                target: "edge_effect_top";
            }
            program { name: "glow_top_hide";
                signal: "glow,hide";
